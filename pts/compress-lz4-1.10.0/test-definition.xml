<?xml version="1.0"?>
<!--Phoronix Test Suite v10.8.5-->
<PhoronixTestSuite>
  <TestInformation>
    <Title>LZ4 Compression</Title>
    <AppVersion>1.10</AppVersion>
    <Description>This test measures the time needed to compress/decompress a sample file (silesia archive) using LZ4 compression.</Description>
    <ResultScale>MB/s</ResultScale>
    <Proportion>HIB</Proportion>
    <TimesToRun>3</TimesToRun>
  </TestInformation>
  <TestProfile>
    <Version>1.10.0</Version>
    <SupportedPlatforms>Linux, BSD, MacOSX, Solaris, Windows</SupportedPlatforms>
    <SoftwareType>Utility</SoftwareType>
    <TestType>Processor</TestType>
    <License>Free</License>
    <Status>Verified</Status>
    <ExternalDependencies>build-utilities</ExternalDependencies>
    <EnvironmentSize>2000</EnvironmentSize>
    <ProjectURL>https://lz4.org/</ProjectURL>
    <RepositoryURL>https://github.com/lz4/lz4</RepositoryURL>
    <Maintainer>Michael Larabel</Maintainer>
  </TestProfile>
  <TestSettings>
    <Default>
      <Arguments>-q -i20</Arguments>
    </Default>
    <Option>
      <DisplayName>Compression Level</DisplayName>
      <Identifier>compression</Identifier>
      <Menu>
        <Entry>
          <Name>1</Name>
          <Value>-b1 -e1</Value>
        </Entry>
        <Entry>
          <Name>2</Name>
          <Value>-b2 -e2</Value>
        </Entry>
        <Entry>
          <Name>3</Name>
          <Value>-b3 -e3</Value>
        </Entry>
        <Entry>
          <Name>9</Name>
          <Value>-b9 -e9</Value>
        </Entry>
        <Entry>
          <Name>12</Name>
          <Value>-b12 -e12</Value>
        </Entry>
      </Menu>
    </Option>
  </TestSettings>
</PhoronixTestSuite>
